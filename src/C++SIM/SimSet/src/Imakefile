LIB	= $(LIBSIMSET)

SOURCES = Head.cc \
	  Link.cc \
	  Linkage.cc

OBJECTS = Head.o \
	  Link.o \
	  Linkage.o

SetLocalCppFlags(-I$(ARJUNA_INCLUDES) -I../include)

AllTarget($(LIB))

MakeLibrary($(LIB),$(OBJECTS))

MakeObjectCPLUSPLUS(Head,NOPARAM)
MakeObjectCPLUSPLUS(Link,NOPARAM)
MakeObjectCPLUSPLUS(Linkage,NOPARAM)
