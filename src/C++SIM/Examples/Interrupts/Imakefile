SOURCES  = Arrivals.cc \
	   Job.cc \
	   MachineShop.cc \
	   Main.cc \
	   Processor.cc \
	   Queue.cc \
	   Signaller.cc

OBJECTS  = Arrivals.o \
	   Job.o \
	   MachineShop.o \
	   Main.o \
	   Processor.o \
	   Queue.o \
	   Signaller.o

LIBRARIES = $(LIBEVENT) $(LIBCPPSIM) $(LIBCONTRIB) $(LIBCPPSIM) $(LIBCONTRIB) $(LIBCOMMON)

SetLocalCppFlags(-I$(ARJUNA_INCLUDES))

AllTarget(Main)

MakeProgramCPLUSPLUS(Main,$(OBJECTS) $(LIBRARIES),NOPARAM)

MakeObjectCPLUSPLUS(Arrivals,NOPARAM)
MakeObjectCPLUSPLUS(Job,NOPARAM)
MakeObjectCPLUSPLUS(MachineShop,NOPARAM)
MakeObjectCPLUSPLUS(Main,NOPARAM)
MakeObjectCPLUSPLUS(Processor,NOPARAM)
MakeObjectCPLUSPLUS(Queue,NOPARAM)
MakeObjectCPLUSPLUS(Signaller,NOPARAM)

TidyTarget(Main)
