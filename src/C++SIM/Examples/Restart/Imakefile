SOURCES  = Tester.cc \
	   Harness.cc \
	   Main.cc

OBJECTS  = Tester.o \
	   Harness.o \
	   Main.o

LIBRARIES = $(LIBCONTRIB) $(LIBCPPSIM) $(LIBCONTRIB) $(LIBCPPSIM) $(LIBCOMMON)

SetLocalCppFlags(-I$(ARJUNA_INCLUDES))

AllTarget(Main)

MakeProgramCPLUSPLUS(Main,$(OBJECTS) $(LIBRARIES),NOPARAM)

MakeObjectCPLUSPLUS(Main,NOPARAM)
MakeObjectCPLUSPLUS(Tester,NOPARAM)
MakeObjectCPLUSPLUS(Harness,NOPARAM)

TidyTarget(Main)
