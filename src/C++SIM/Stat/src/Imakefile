LIB	= $(LIBSTAT)

SOURCES = Mean.cc \
	  Pareto.cc \
	  Variance.cc \
	  TimeVariance.cc \
	  PHistogram.cc \
	  Histogram.cc \
	  SHistogram.cc \
	  Quantile.cc

OBJECTS = Mean.o \
	  Pareto.o \
	  Variance.o \
	  TimeVariance.o \
	  PHistogram.o \
	  Histogram.o \
	  SHistogram.o \
	  Quantile.o

SetLocalCppFlags(-I$(ARJUNA_INCLUDES) -I../include)

AllTarget($(LIB))

MakeLibrary($(LIB),$(OBJECTS))

MakeObjectCPLUSPLUS(Mean,NOPARAM)
MakeObjectCPLUSPLUS(Pareto,NOPARAM)
MakeObjectCPLUSPLUS(Variance,NOPARAM)
MakeObjectCPLUSPLUS(TimeVariance,NOPARAM)
MakeObjectCPLUSPLUS(Histogram,NOPARAM)
MakeObjectCPLUSPLUS(PHistogram,NOPARAM)
MakeObjectCPLUSPLUS(SHistogram,NOPARAM)
MakeObjectCPLUSPLUS(Quantile,NOPARAM)
