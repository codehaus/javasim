SOURCES  = TestHistogram.cc \
	   TestPareto.cc

OBJECTS  = TestHistogram.o \
	   TestPareto.o

LIBRARIES = $(LIBSTAT) $(LIBCPPSIM) $(LIBCOMMON)

SetLocalCppFlags(-I$(ARJUNA_INCLUDES))

AllTarget(TestHistogram TestPareto)

MakeProgramCPLUSPLUS(TestHistogram,TestHistogram.o $(LIBRARIES),NOPARAM)
MakeProgramCPLUSPLUS(TestPareto,TestPareto.o $(LIBRARIES),NOPARAM)

MakeObjectCPLUSPLUS(TestHistogram,NOPARAM)
MakeObjectCPLUSPLUS(TestPareto,NOPARAM)

TidyTarget(TestHistogram TestPareto)
