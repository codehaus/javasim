SOURCES  = TestRandom1.cc \
	   TestRandom2.cc

OBJECTS  = TestRandom1.o \
	   TestRandom2.o

LIBRARIES = $(LIBCPPSIM) $(LIBCOMMON)

SetLocalCppFlags(-I$(ARJUNA_INCLUDES))

AllTarget(TestRandom1 TestRandom2)

MakeProgramCPLUSPLUS(TestRandom1,TestRandom1.o $(LIBRARIES),NOPARAM)
MakeProgramCPLUSPLUS(TestRandom2,TestRandom2.o $(LIBRARIES),NOPARAM)

MakeObjectCPLUSPLUS(TestRandom1,NOPARAM)
MakeObjectCPLUSPLUS(TestRandom2,NOPARAM)

TidyTarget(TestRandom1 TestRandom2)
