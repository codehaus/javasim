SOURCES  = Timer.cc \
	   Tester.cc \
	   DummyProcess.cc

OBJECTS  = Timer.o \
	   Tester.o \
	   DummyProcess.o

LIBRARIES = $(LIBCONTRIB) $(LIBCPPSIM) $(LIBCONTRIB) $(LIBCPPSIM) $(LIBSTAT) $(LIBCOMMON)

#ifdef Calendar_Queue
SetLocalCppFlags(-I$(ARJUNA_INCLUDES) -I../../ClassLib/include -I../../Contrib/Calendar/include)
#else
SetLocalCppFlags(-I$(ARJUNA_INCLUDES) -I../../ClassLib/include)
#endif

AllTarget(Timer)

MakeProgramCPLUSPLUS(Timer,$(OBJECTS) $(LIBRARIES),NOPARAM)

MakeObjectCPLUSPLUS(Timer,NOPARAM)
MakeObjectCPLUSPLUS(Tester,NOPARAM)
MakeObjectCPLUSPLUS(DummyProcess,NOPARAM)

TidyTarget(Timer)
